<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
       "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
 
  <field name="phone">   
      	<field-validator type="conditionalvisitor">
         	<message>Phone required </message>
        </field-validator>
  </field>
  
  <field name="email">   
      	<field-validator type="conditionalvisitor">
        	 <message>emailrequired </message>
         </field-validator>
  </field>
  
  <field name="fax">   
      	<field-validator type="conditionalvisitor">
         	<message>Fax required </message>
        </field-validator>
  </field>
  
   <field name="delivery">   
      <field-validator type="conditionalvisitor">
         <message>Delivery required </message>
      </field-validator>
  </field>
  
  <field name="orders">   
      <field-validator type="conditionalvisitor">
         <message>Online Order Monitoring Service required </message>
      </field-validator>
  </field>
   <field name="updateOrders">   
      <field-validator type="conditionalvisitor">
         <message>Update Order Service Required</message>
      </field-validator>
  </field>  
</validators>
